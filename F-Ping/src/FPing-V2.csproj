<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
    <WebView2DispatchAdapterUseJavaScriptCase>False</WebView2DispatchAdapterUseJavaScriptCase>
    <WV2WinRTRequireAllowForWebAttribute>True</WV2WinRTRequireAllowForWebAttribute>
  </PropertyGroup>

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>FPing_V2</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>False</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="public\**" />
    <EmbeddedResource Remove="public\**" />
    <None Remove="public\**" />
    <Page Remove="public\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="F-Source\data\en.json" />
    <None Remove="F-Source\data\es.json" />
    <None Remove="F-Source\data\news.json" />
    <None Remove="F-Source\data\servers.json" />
    <None Remove="F-Source\imgs\icons\arror.png" />
    <None Remove="F-Source\imgs\icons\continue.png" />
    <None Remove="F-Source\imgs\icons\cry.png" />
    <None Remove="F-Source\imgs\icons\home.png" />
    <None Remove="F-Source\imgs\icons\LoadingBar.png" />
    <None Remove="F-Source\imgs\icons\save.png" />
    <None Remove="F-Source\imgs\icons\settings.png" />
    <None Remove="F-Source\imgs\icons\shop.png" />
    <None Remove="F-Source\imgs\icons\theme.png" />
    <None Remove="F-Source\imgs\icons\user.png" />
    <None Remove="F-Source\imgs\icons\vur.png" />
    <None Remove="F-Source\imgs\icons\wif.png" />
    <None Remove="F-Source\imgs\icons\wiput.png" />
    <None Remove="F-Source\imgs\logo.png" />
    <None Remove="F-Source\imgs\Ramirez.png" />
    <None Remove="F-Source\index.html" />
    <None Remove="F-Source\main.js" />
    <None Remove="F-Source\shop.js" />
    <None Remove="F-Source\status.js" />
    <None Remove="F-Source\style.css" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="F-Source\data\en.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="F-Source\data\es.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="F-Source\data\news.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="F-Source\data\servers.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="F-Source\imgs\icons\arror.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="F-Source\imgs\icons\continue.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
